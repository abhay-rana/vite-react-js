{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:import/recommended",
        "plugin:jsx-a11y/recommended",
        "eslint-config-prettier"
    ],
    "env": {
        // this is for the "document" and "window" global object
        "browser": true,
        "node": true
    },
    "rules": {
        "no-unused-vars": "warn",
        "import/no-unresolved": "off",
        "react/react-in-jsx-scope": "off"
    }
}
